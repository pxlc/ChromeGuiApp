    "user_info_cache": {
        "_type_": "single_path_create",
        "win32": [
            "${HOME}/.pxlc_ChromeGuiApp",
            "${USERPROFILE}/AppData/Local/pxlc_ChromeGuiApp"
        ],
        "default": [ "${HOME}/.pxlc_ChromeGuiApp" ]
    },
    "user_temp_root": {
        "_type_": "single_path_create",
        "win32": [
            "${TEMP}/pxlc_ChromeGuiApp_temp_${USERNAME}",
            "${USERPROFILE}/AppData/Local/Temp/pxlc_ChromeGuiApp_temp"
        ],
        "default": [ "/usr/tmp/pxlc_ChromeGuiApp_temp_${USER}" ]
    },

    "port_cycle_range": {"start": 40000, "end": 42000},

    "chrome_exe_path": {
        "_type_": "single_path",
        "win32": "C:/Program Files (x86)/Google/Chrome/Application/chrome.exe"
    },

    "python_exe_path": {
        "_type_": "single_path",
        "win32": "C:/Python27/python.exe"
    },

    "pythonpath_additions": {
        "_type_": "multi_path",
        "win32": [
        ],
        "default": []
    }
}
